{
  "name": "creatorflow-studio",
  "version": "1.0.0",
  "description": "AI-powered content creation platform",
  "main": "server.js",
  "type": "module",
"scripts": {
  "start": "node server.js",
  "dev": "node server.js",
  "lint": "eslint server.js lib --ext .js",
  "typecheck": "tsc --noEmit",
  "test": "NODE_ENV=test SESSION_SECRET=test-session-secret GOOGLE_CLIENT_ID=test-google-client-id GOOGLE_CLIENT_SECRET=test-google-client-secret GOOGLE_CALLBACK_URL=http://localhost:3000/auth/google/callback FACEBOOK_APP_ID=test-facebook-app-id FACEBOOK_APP_SECRET=test-facebook-app-secret FACEBOOK_CALLBACK_URL=http://localhost:3000/auth/facebook/callback node --test",
  "build:static": "node scripts/sync-static.js",
  "build": "npm run build:static"
}
  }
  "dependencies": {
    "cors": "2.8.5",
    "csurf": "1.11.0",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "connect-redis": "7.1.1",
    "multer": "2.0.2",
    "passport": "0.7.0",
    "passport-facebook": "3.0.0",
    "passport-google-oauth20": "2.0.0",
    "redis": "4.7.0",
    "openai": "4.82.0",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "8.46.4",
    "@typescript-eslint/parser": "8.46.4",
    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-unicorn": "53.0.0",
    "prettier": "3.6.2",
    "supertest": "7.0.0",
    "typescript": "5.9.3"
  },
  "keywords": [
    "ai",
    "content",
    "creation",
    "studio"
  ],
  "author": "CreatorFlow Team",
  "license": "MIT"
}
